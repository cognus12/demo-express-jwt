{
  "name": "express-ts-starter",
  "version": "1.0.0",
  "scripts": {
    "build-ts": "tsc",
    "build": "npm run build-ts && npm run lint",
    "lint": "tsc --noEmit && eslint \"./app/**/*.{js,ts}\" --quiet --fix",
    "serve": "node dist/server.js",
    "start": "npm run serve",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "watch": "concurrently \"npm run watch-ts\" \"npm run watch-node\"",
    "dev": "npm run watch",
    "prod": "npm run build && npm run start",
    "test": "jest --runInBand"
  },
  "author": "Ivan Zotov (corvuss90@gmail.com)",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.11.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.11",
    "ms-typescript": "^2.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/eslint": "^7.2.11",
    "@types/express": "^4.17.11",
    "@types/express-flash": "0.0.2",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^15.6.0",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "concurrently": "^6.1.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "ts-jest": "^27.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "main": "index.js",
  "keywords": [],
  "description": "Simple Express.js example written in TypeScript. Can be used as starter template for REST API development"
}
